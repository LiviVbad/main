<prism:PrismApplication
    x:Class="AppFrameworkDemo.Shared.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:converters="clr-namespace:AppFrameworkDemo.Shared.Converters;assembly=AppFrameworkDemo.Shared"
    xmlns:prism="http://prismlibrary.com"
    xmlns:sync="clr-namespace:Syncfusion.XForms.Themes;assembly=Syncfusion.Core.XForms">
    <Application.Resources>
        <ResourceDictionary>
            <sync:LightTheme />
            <sync:DarkTheme />

            <converters:DatetimeConverter x:Key="DatetimeConverter" />
            <converters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
            <converters:BoolToYesNoConverter x:Key="BoolToYesNoConverter" />
            <converters:ImageSourceConverter x:Key="ImageSourceConverter" />
            <converters:BackgroundColorConverter x:Key="BackgroundColorConverter" />
            <converters:IndentConverter x:Key="IndentConverter" />
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>